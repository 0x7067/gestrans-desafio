{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["./*"],
			"@/components/*": ["./components/*"],
			"@/types/*": ["./types/*"],
			"@/services/*": ["./services/*"],
			"@/utils/*": ["./utils/*"],
			"@/constants/*": ["./constants/*"],
			"@/hooks/*": ["./hooks/*"]
		}
	},
	"include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"],
	"exclude": ["node_modules"]
}
